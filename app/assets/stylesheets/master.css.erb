/* CSS RESET ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
*/

html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp, small, strong, sub, sup, var, b, i, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, figure, footer, header, hgroup, menu, nav, section, menu, time, mark, audio, video {
  margin: 0; padding: 0;
  border: 0; outline: 0;
  font-size: 100%; vertical-align: baseline;
  background: transparent;
}

article, aside, figure, footer, header, hgroup, nav, section {display: block;}
img, object, embed {max-width: 100%;}
html {overflow-y: scroll;} /* force a vertical scrollbar to prevent a jumpy page */
ul {list-style: none;}
blockquote, q {quotes: none;}
blockquote:before, blockquote:after, q:before, q:after {content: ''; content: none;}
a {margin: 0; padding: 0; font-size: 100%; vertical-align: baseline; background: transparent;}
del {text-decoration: line-through;}
abbr[title], dfn[title] {border-bottom: 1px dotted #000; cursor: help;}
table {border-collapse: collapse; border-spacing: 0;} /* tables still need cellspacing="0" in the markup */
th {font-weight: bold; vertical-align: bottom;}
td {font-weight: normal; vertical-align: top;}
hr {display: block; height: 1px; border: 0; border-top: 1px solid #ccc; margin: 1em 0; padding: 0;}
input, select {vertical-align: middle;}
pre { white-space: pre; /* CSS2 */ white-space: pre-wrap; /* CSS 2.1 */ white-space: pre-line; /* CSS 3 (and 2.1 as well, actually) */ word-wrap: break-word; /* IE */ }
input[type="radio"] {vertical-align: text-bottom;}
input[type="checkbox"] {vertical-align: bottom; *vertical-align: baseline;}
.ie6 input {vertical-align: text-bottom;}
select, input, textarea {font: 99% sans-serif;}
table {font-size: inherit; font: 100%;}
 a:hover, a:active {outline: none;} /* Accessible focus treatment people.opera.com/patrickl/experiments/keyboard/test */
small {font-size: 85%;}
strong, th {font-weight: bold;}
td, td img {vertical-align: top;} 
sub, sup {font-size: 75%; line-height: 0; position: relative;} sup {top: -0.5em;} sub {bottom: -0.25em;}
pre, code, kbd, samp {font-family: monospace, sans-serif;} /* standardize any monospaced elements */
.clickable, label, input[type=button], input[type=submit], button {cursor: pointer;} /* hand cursor on clickable elements */
button, input, select, textarea {margin: 0;} /* Webkit browsers add a 2px margin outside the chrome of form elements */
button {width: auto; overflow: visible;} /* make buttons play nice in IE */
.ie7 img {-ms-interpolation-mode: bicubic;} /* scale images in IE7 more attractively */
.ie6 html {filter: expression(document.execCommand("BackgroundImageCache", false, true));} /* prevent BG image flicker upon hover */

/* let's clear some floats */
.clearfix:before, .clearfix:after { content: "\0020"; display: block; height: 0; overflow: hidden; }  
.clearfix:after { clear: both; }  
.clearfix { zoom: 1; }  

/* new Linux- and Windows-friendly sans-serif font stack: http://mky.be/fontstack */
body {font: 13px Helvetica, Arial, sans-serif;}
/* Custom text-selection colors (remove any text shadows: twitter.com/miketaylr/status/12228805301) */
ins {background-color: #fcd700; color: #000; text-decoration: none;}
mark {background-color: #220cfc; color: #000; font-style: italic; font-weight: bold;}


/* BASE STYLES ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
*/

html, body {
  height: 100%; width: 100%;
  font: normal normal normal 100%/1.25em Helvetica, Arial, sans-serif; color: #333; vertical-align: baseline;
}

body { background: #F7F7F7; }

h1, h2, h3, h4, h5, h6 { font: normal normal normal 1em/1em Helvetica, Arial, sans-serif; } /* Base heading styles */

h1 { font: normal normal bold 2em/40px Helvetica, Arial, sans-serif; color: #333; }
h2 { font: normal normal bold 1.5em/30px Helvetica, Arial, sans-serif; color: #333; }
h3 { font: normal normal bold 1.25em/30px Helvetica, Arial, sans-serif; color: #333; }

p { } /* Paragraph style. Use for text, or for a libe break. </br> is for pebs, and you're not a pleb right? */
span { } /* Use as a default container for text that requires a Class or ID */
a, a:visited { font: normal normal bold 1em Helvetica, Arial, sans-serif; color: #000; text-decoration: none; cursor: pointer; }
a:hover { text-decoration: underline; } /* Basic link feedback for user */

ul {   }
li {   }

label { font: normal normal normal 1em/1em Helvetica, Arial, sans-serif; color: #333; white-space: nowrap; cursor: pointer; }

input, textarea { padding: 0 10px;  height: 30px; border: 1px solid #D9D9D9; resize: none; font: normal normal normal 1em/1em Helvetica, Arial, sans-serif; color: #666; }
input:hover, textarea:hover { border-color: #B9B9B9; }
input:focus, textarea:focus { outline: none; border-color: #3079ED; }

/* BASE STYLES - ALERTS / ERRORS
/////////////////////////////////////////////////////////////////////////////////////////////////*/

/* Base notification container for users. -- Currently looking into a JS popdown notification */
#notice {
  margin: 20px; padding: 0 30px; height: 34px; width: auto; clear: both;
  font: normal normal normal 1em/36px Helvetica, Arial, sans-serif; color: #F00; text-align: center;
  background: #F3F8FC; border: #B2D1FF solid 1px;
}

#alert {
  padding: 18px 20px 18px 40px; margin: 10px 20px;
  font-size: 0.9em; color: #F00;
  background: #ffcccc url(<%= asset_path 'exclamation.png' %>) no-repeat 10px center; border: red solid 1px;
}

/* BASE STYLES - STRUCTURE MODULES
/////////////////////////////////////////////////////////////////////////////////////////////////*/

/* Apply these class modules to elements. Any more than 3 is overkill and you should probably have a specific container. */

.floatleft { float: left; } /* Float an element to the left */

.floatright { float: right; } /* Float an element to the right */

.row { margin: 0 0 18px 0; } /* Basic row / horizontal container */

.fields {
  margin: 0 0 18px 0;
  display: -moz-inline-stack; display: inline-block; zoom: 1; *display: inline; vertical-align: top; /* Cross-browser Inline-block */
}

/* BASE STYLES - TEXT MODULES
/////////////////////////////////////////////////////////////////////////////////////////////////*/

/* Apply these to specific font containers. Any more than 2 is overkill and a specific style / container should be used */

.details { font: normal normal normal 0.75em/18px Helvetica, Arial, sans-serif; color: #666; }

.bold { font-weight: bold; }

.textalignleft { text-align: left; } /* Align text to the left */

.textalignright { text-align: right; } /* Align text to the right */

.textaligncenter { text-align: center; } /* Align text to the center */

.truncate { white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }

.required { color: #E25358; }

/* BUTTONS ///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
*/

input[button], button { /* Default button style */
  padding: 5px 10px; margin: 0; height: 30px;
  font: normal normal normal 14px/20px Helvetica, Arial, sans-serif; text-decoration: none;
  display: -moz-inline-stack; display: inline-block; zoom: 1; *display: inline; vertical-align: top; /* Cross-browser Inline-block */
  background-color: #FFFFFF; border: 1px solid #E0E0E0; border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#F7F7F7), to(#EDEDED)); /* Chrome,Safari4+ */
  background-image: -webkit-linear-gradient(top, #F7F7F7, #EDEDED); /* Chrome10+,Safari5.1+ */
  background-image: -moz-linear-gradient(top, #F7F7F7, #EDEDED); /* FF3.6+ */
  background-image: -ms-linear-gradient(top, #F7F7F7, #EDEDED); /* IE10+ */
  background-image: -o-linear-gradient(top, #F7F7F7, #EDEDED); /* Opera11.10+ */
  background-image: linear-gradient(top, #F7F7F7, #EDEDED); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F7F7F7', endColorstr='#EDEDED', GradientType=0); /* IE6-9 */
  cursor: pointer;
}

input[button]:hover, button:hover { /* Default Button hover style */
  background-color: #357ae8; border:1px solid #C1C1C1;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#E5E5E5), to(#EDEDED));
  background-image: -webkit-linear-gradient(top, #E5E5E5, #EDEDED);
  background-image: -moz-linear-gradient(top, #F7F7F7, #E5E5E5);
  background-image: -ms-linear-gradient(top, #F7F7F7, #E5E5E5);
  background-image: -o-linear-gradient(top, #F7F7F7, #E5E5E5);
  background-image: linear-gradient(top, #F7F7F7, #E5E5E5);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr = '#F7F7F7', endColorstr = '#E5E5E5', GradientType=0);
}

input[button]:active, button:active, .button:active {
  box-shadow: inset 0px 1px 2px rgba(0, 0, 0, 0.1); -moz-box-shadow: inset 0px 1px 2px rgba(0, 0, 0, 0.1); -webkit-box-shadow: inset 0px 1px 2px rgba(0, 0, 0, 0.1);
}

input[disabled], input[disabled]:hover { /* Disabled Button Style */
  padding: 5px 10px; margin: 0; height: 30px;
  font: normal normal normal 14px/36px Helvetica, Arial, sans-serif; text-decoration: none; color: #CCC;
  display: -moz-inline-stack; display: inline-block; zoom: 1; *display: inline; vertical-align: top; /* Cross-browser Inline-block */
  background-color: #FFFFFF; border: 1px solid #E0E0E0; border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#F7F7F7), to(#EDEDED)); /* Chrome,Safari4+ */
  background-image: -webkit-linear-gradient(top, #F7F7F7, #EDEDED); /* Chrome10+,Safari5.1+ */
  background-image: -moz-linear-gradient(top, #F7F7F7, #EDEDED); /* FF3.6+ */
  background-image: -ms-linear-gradient(top, #F7F7F7, #EDEDED); /* IE10+ */
  background-image: -o-linear-gradient(top, #F7F7F7, #EDEDED); /* Opera11.10+ */
  background-image: linear-gradient(top, #F7F7F7, #EDEDED); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#F7F7F7', endColorstr='#EDEDED', GradientType=0); /* IE6-9 */
  cursor: default;
}

input[disabled]:active { box-shadow: none; -moz-box-shadow: none; -webkit-box-shadow: none; } /* Disabled Button Style */

/* BUTTONS - BASE
/////////////////////////////////////////////////////////////////////////////////////////////////*/

.button { /* Default Button Style - Gray */
  padding: 5px 10px; margin: 0 10px 0 0; height: 18px;
  display: -moz-inline-stack; display: inline-block; zoom: 1; *display: inline; vertical-align: top; /* Cross-browser Inline-block */
  font: normal normal normal 14px/18px Helvetica, Arial, sans-serif; text-decoration: none;
