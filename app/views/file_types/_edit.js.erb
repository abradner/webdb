$('#wizard').jqmHide();
$('#wizard').html('<%= escape_javascript(render :partial => 'file_types/form' ) %>');
$('#wizard').jqmShow();
$('[id^=show_][id$=_wizard]').removeClass('ac_loading');
$('#metadata_checkbox_container :checkbox').iphoneStyle({ checkedLabel: 'Yes', uncheckedLabel: 'No' ,
    onChange: function(elem, value) {
        $("#save_button").attr("value", value ? "Save Define Metadata" : "Save and Close");
    }
});